{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [ "./src/*" ],
      "@components/*": [ "./src/components/*" ],
      "@images/*": [ "./src/images/*" ],
      "@layouts/*": [ "./src/layouts/*" ],
      "@pages/*": [ "./src/pages/*" ],
      "@styles/*": [ "./src/styles/*" ],
      "@studio/*": [ "./studio/*" ]
    }
  }
}